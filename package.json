{
  "name": "@signageos/smil-player",
  "version": "1.4.0",
  "description": "SMIL player",
  "main": "dist/index.html",
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "package.json"
  ],
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.10.2"
  },
  "scripts": {
    "test": "nyc --check-coverage mocha  -- \"test/**/*.spec.ts\" --require ts-node/register -P tsconfig.json",
    "e2e": "npm run start-e2e-server && cypress run --headless && npm run stop-e2e-server",
    "e2e-open": "npm run start-e2e-server && cypress open && npm run stop-e2e-server",
    "start-e2e-server": "tsc cypress/tools/localServer.ts && node cypress/tools/localServer.js &",
    "stop-e2e-server": "kill $(lsof -t -i:3000)",
    "lint": "tslint --config node_modules/@signageos/codestyle/tslint.json \"{src,test}/**/*.{ts,tsx}\"",
    "lint-fix": "tslint --config node_modules/@signageos/codestyle/tslint.json \"{src,test}/**/*.{ts,tsx}\" --fix",
    "upload-applet": "sos applet upload",
    "clean": "rm -rf cache dist/*",
    "prepare": "npm run clean && npm run build",
    "build": "npm run clean && webpack --display-modules",
    "build-test": "tsc",
    "start": "webpack-dev-server --mode development --open-emulator",
    "start-cypress": "webpack-dev-server --mode none --open-emulator",
    "escheck": "es-check --module es5 dist/index.js",
    "check": "depcheck --specials=tslint,webpack,mocha --parsers='*.ts:typescript,*.js:es6' --detectors='requireCallExpression,importDeclaration' --ignore-dirs='dist,packages' --ignores='@types/*,@signageos/codestyle,depcheck,webpack-cli,mocha,nyc,@babel/preset-env,babel-loader,cache-loader,es-check,webpack-dev-server'"
  },
  "author": "signageOS <dev@signageos.io>",
  "repository": {
    "url": "git@gitlab.com:signageos/smil-player",
    "type": "git"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "7.9.0",
    "@signageos/codestyle": "0.0.15",
    "@signageos/front-applet": "5.0.0-beta.6",
    "@signageos/front-display": "8.7.0",
    "@signageos/webpack-plugin": "^0.1.1",
    "@types/async": "^3.2.0",
    "@types/chai": "4.1.7",
    "@types/debug": "^4.1.5",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.14.5",
    "@types/xml2js": "^0.4.5",
    "@types/xmldom": "^0.1.29",
    "@types/express": "^4.17.11",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "8.1.0",
    "cache-loader": "4.1.0",
    "cors-anywhere": "0.4.1",
    "cypress": "^6.2.1",
    "cypress-iframe": "^1.0.1",
    "depcheck": "0.8.3",
    "es-check": "5.2.0",
    "html-webpack-plugin": "^3.2.0",
    "tslint": "6.1.1",
    "typescript": "3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "3.11.0"
  },
  "dependencies": {
    "@signageos/front-applet-extension-nexmosphere": "^0.1.3",
    "@types/object-hash": "^2.1.0",
    "async": "^3.2.0",
    "chai": "^4.2.0",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "is-url-superb": "^3.0.0",
    "json-easy-filter": "^0.3.1",
    "lodash": "^4.17.15",
    "mocha": "^7.1.1",
    "moment": "^2.26.0",
    "nyc": "^15.0.0",
    "object-hash": "^2.1.1",
    "split-string": "^6.1.0",
    "ts-node": "^8.8.1",
    "whatwg-fetch": "^3.4.0",
    "xml2js": "^0.4.23"
  },
  "sos": {
    "appletUid": "ae831411425df581cae9d74c2a8c04386166d0cbb70ef377f2"
  }
}
